			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"if (pm.response.code === 200) {",
							"    const userId = pm.response.json().data.userId;",
							"    pm.collectionVariables.set('user_id', userId);",
							"} else {",
							"    console.log(\"Failed to register user.\");",
							"}"
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
